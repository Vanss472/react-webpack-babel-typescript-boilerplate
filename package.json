{
  "name": "react-webpack-babel-typescript-boilerplate",
  "version": "1.0.0",
  "description": "A simple starter to get up and developing quickly with React",
  "main": "index.js",
  "author": "Vanessa Martinez",
  "license": "ISC",
  "private": true,
  "scripts": {
    "develop": "webpack-dev-server --open",
    "build": "rm -rf ./dist && webpack --mode=development",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js"
